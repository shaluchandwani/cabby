{
  "name": "Cabby",
  "version": "1.0.0",
  "description": "A place for all your travel solutions",
  "main": "index.js",
  "scripts": {
    "test": "export NODE_ENV=test && nyc --reporter=html --reporter=text mocha  --require  @babel/register src/tests/*.js --timeout 100000 --exit",
    "start": "node production/index.js",
    "dev": "nodemon --exec babel-node src/server.js",
    "seq": "npm run sequelize",
    "npminstall": "curl -o- https://raw.githubusercontent.com/creationix/nvm/v0.34.0/install.sh | bash",
    "npmrunversion": "npm install -g npm@latest",
    "migrate:up": "npx sequelize-cli db:migrate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate:drop": "sequelize db:migrate:undo:all",
    "pretest": "npm run migrate:drop && npm run migrate:up"
  },
  "author": "Shalu Chandwani",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@hapi/joi": "^17.1.0",
    "babel-eslint": "^10.0.3",
    "bcrypt": "^5.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1"
  }
}
